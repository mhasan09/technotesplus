\n                await router.push({name: 'Login'})\r\n            } catch(err) {\r\n              console.log(err);\r\n            } \r\n      }\r\n\r\n      return {\r\n        state,\r\n        handleCreateUser\r\n    }\r\n\r\n  }\r\n}\r\n  \r\n\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]}]}